{
  "name": "aurelia-electron-webpack",
  "productName": "Aurelia + Electron + Webpack",
  "description": "Aurelia Electron app built with Webpack",
  "version": "0.1.0",
  "author": "Tim Fish <tim@timfish.uk>",
  "repository": {
    "type": "git",
    "url": "https://github.com/timfish/aurelia-electron-webpack.git"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ./dist/ --debug",
    "build": "cross-env TS_NODE_PRETTY=true TS_NODE_PROJECT=./build/tsconfig.json webpack --config ./build/webpack.config.ts --env.production",
    "build:dev": "cross-env TS_NODE_PRETTY=true TS_NODE_PROJECT=./build/tsconfig.json webpack --config ./build/webpack.config.ts",
    "watch": "yarn build:dev && cross-env TS_NODE_PRETTY=true TS_NODE_PROJECT=./build/tsconfig.json webpack-dev-server --config ./build/webpack.config.ts --hot --inline",
    "release": "electron-builder --config ./build/release.yml",
    "fix": "prettier --write \"./{src,build}/**/*.ts\" && tslint --fix --force --format stylish \"./{src,build}/**/*.ts\""
  },
  "main": "main.js",
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "aurelia-bootstrapper": "^2.2.0",
    "aurelia-dependency-injection": "^1.4.1",
    "aurelia-framework": "^1.3.0",
    "aurelia-pal": "^1.8.0",
    "aurelia-path": "^1.1.1",
    "aurelia-templating": "^1.10.0",
    "usb-detection": "^4.0.0"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.2",
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^10.11.3",
    "@types/webpack": "^4.4.13",
    "@types/webpack-bundle-analyzer": "^2.13.0",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-merge": "^4.1.3",
    "aurelia-webpack-plugin": "^3.0.0",
    "awesome-node-loader": "^1.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "electron": "^2.0.0",
    "electron-builder": "^20.28.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.8.1",
    "node-sass": "^4.9.3",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.0.3",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "write-file-webpack-plugin": "^4.4.1"
  }
}
