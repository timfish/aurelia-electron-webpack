{
  "name": "aurelia-electron-webpack",
  "productName": "Aurelia + Electron + Webpack",
  "description": "Aurelia Electron app built with Webpack",
  "version": "0.1.0",
  "author": "Tim Fish <tim@timfish.uk>",
  "repository": {
    "type": "git",
    "url": "https://github.com/timfish/aurelia-electron-webpack.git"
  },
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "electron ./dist/ --debug",
    "build": "yarn build:main && yarn build:renderer",
    "build:main": "cross-env TS_NODE_PROJECT=./build/tsconfig.json webpack --config ./build/webpack.main.config.ts --env.production",
    "build:renderer": "cross-env TS_NODE_PROJECT=./build/tsconfig.json webpack --config ./build/webpack.renderer.config.ts --env.production",
    "build:dev": "yarn build:dev:main && yarn build:dev:renderer",
    "build:dev:main": "cross-env TS_NODE_PROJECT=./build/tsconfig.json webpack --config ./build/webpack.main.config.ts --progress",
    "build:dev:renderer": "cross-env TS_NODE_PROJECT=./build/tsconfig.json webpack --config ./build/webpack.renderer.config.ts --progress",
    "watch": "yarn build:dev:main && cross-env TS_NODE_PROJECT=./build/tsconfig.json webpack-dev-server --config ./build/webpack.renderer.config.ts --hot --inline --progress",
    "release": "electron-builder --config ./build/release.yml",
    "fix": "prettier --write \"./{src,build}/**/*.ts\" && tslint --fix --force --format stylish \"./{src,build}/**/*.ts\""
  },
  "main": "main.js",
  "license": "MIT",
  "engines": {
    "node": ">= 8.0.0"
  },
  "dependencies": {
    "aurelia-binding": "^2.2.0",
    "aurelia-bootstrapper": "^2.3.2",
    "aurelia-dependency-injection": "^1.4.2",
    "aurelia-framework": "^1.3.1",
    "aurelia-pal": "^1.8.1",
    "aurelia-pal-browser": "^1.8.1",
    "aurelia-path": "^1.1.2",
    "aurelia-templating": "^1.10.1",
    "usb-detection": "^4.1.0"
  },
  "devDependencies": {
    "@types/copy-webpack-plugin": "^4.4.2",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/node": "^10.11.3",
    "@types/webpack": "^4.4.13",
    "@types/webpack-bundle-analyzer": "^2.13.0",
    "@types/webpack-dev-server": "^3.1.1",
    "@types/webpack-merge": "^4.1.3",
    "@zeit/webpack-asset-relocator-loader": "^0.3.1",
    "aurelia-webpack-plugin": "^3.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "electron": "^4.1.0",
    "electron-builder": "^20.28.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "4.0.0-beta.5",
    "node-sass": "^4.11.0",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.3.3333",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "write-file-webpack-plugin": "^4.5.0"
  }
}
